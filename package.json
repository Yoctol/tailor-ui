{
  "name": "@yoctol/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoctol/ui.git"
  },
  "bugs": {
    "url": "https://github.com/yoctol/ui/issues"
  },
  "version": "2.0.0-alpha.10",
  "main": "lib/index.umd.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:js && npm run build:esm",
    "build:esm": "rollup -c",
    "build:js": "cross-env NODE_ENV=production babel src -d lib --extensions '.ts,.tsx' --ignore **/__tests__",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "commit": "git-cz",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:es": "eslint src --ext=ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged && npm run type-check",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prepublishOnly": "npm run build",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "preversion": "npm test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "@reach/observe-rect": "^1.0.3",
    "immutable": "^3.8.2",
    "moment": "^2.22.2",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "rc-calendar": "^9.7.9",
    "rc-editor-mention": "^1.1.7",
    "rc-time-picker": "^3.4.0",
    "react-powerplug": "^1.0.0-rc.1",
    "react-select": "^2.1.0",
    "react-spring": "^5.9.2",
    "react-textarea-autosize": "^7.0.4",
    "styled-normalize": "^8.0.1",
    "styled-system": "^3.1.6"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-icons": ">=3.1.0",
    "styled-components": ">=3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.4",
    "@types/ramda": "^0.25.39",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-select": "^2.0.4",
    "@types/styled-components": "^3.0.1",
    "@types/styled-system": "^3.0.4",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-typescript-to-proptypes": "^0.9.0",
    "commitizen": "^3.0.2",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.12.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-yoctol": "^0.18.2",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "jest-dom": "^2.0.4",
    "jest-styled-components": "^6.2.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "prettier-package-json": "^2.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-icons": "^3.1.0",
    "react-testing-library": "^5.2.0",
    "rollup": "^0.66.5",
    "rollup-plugin-commonjs": "^9.1.8",
    "styled-components": "^3.4.9",
    "stylelint": "9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^1.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.1",
    "typescript-eslint-parser": "^20.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "typings": "lib/index.d.ts"
}
