{
  "name": "@yoctol/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoctol/ui.git"
  },
  "bugs": {
    "url": "https://github.com/yoctol/ui/issues"
  },
  "version": "1.10.3",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib --ignore **/__tests__,**/__stories__",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "lint:js": "eslint src",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-icons": "^2.2.7",
    "react-powerplug": "^1.0.0-rc.1",
    "react-select": "^2.0.0",
    "react-spring": "^5.6.7",
    "react-textarea-autosize": "^7.0.4",
    "styled-components": "^3.4.4",
    "styled-normalize": "^8.0.0",
    "styled-system": "^3.0.2",
    "styled-tools": "^1.3.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "styled-components": ">=3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-ramda": "^1.6.2",
    "babel-plugin-styled-components": "^1.5.1",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.10.3",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-yoctol": "^0.18.1",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "stylelint": "9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^1.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  }
}
