{
  "name": "@yoctol/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoctol/ui.git"
  },
  "bugs": {
    "url": "https://github.com/yoctol/ui/issues"
  },
  "version": "2.0.0-alpha.2",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:js": "cross-env NODE_ENV=production babel src -d lib --extensions \".ts,.tsx\" --ignore **/__tests__",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "precommit": "npm run lint:staged",
    "commit": "git-cz",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged && npm run type-check",
    "lint:ts": "eslint src --ext=ts,tsx",
    "prepublishOnly": "npm run build",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "preversion": "npm test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "@reach/observe-rect": "^1.0.3",
    "immutable": "^3.8.2",
    "polished": "^2.0.3",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "rc-editor-mention": "^1.1.7",
    "react-icons": "^2.2.7",
    "react-powerplug": "^1.0.0-rc.1",
    "react-select": "^2.0.0",
    "react-spring": "^5.7.2",
    "react-textarea-autosize": "^7.0.4",
    "styled-normalize": "^8.0.0",
    "styled-system": "^3.0.3"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "styled-components": ">=3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/ramda": "^0.25.38",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-icons": "^2.2.6",
    "@types/react-select": "^2.0.3",
    "@types/styled-components": "^3.0.1",
    "@types/styled-system": "^3.0.4",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-styled-components": "^1.6.4",
    "babel-plugin-typescript-to-proptypes": "^0.8.0",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.11.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-yoctol": "^0.18.1",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-dom": "^1.12.0",
    "jest-styled-components": "^6.2.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-testing-library": "^5.0.1",
    "styled-components": "^3.4.6",
    "stylelint": "9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^1.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "typings": "lib/index.d.ts"
}
