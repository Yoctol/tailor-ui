{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-typescript-to-proptypes",
    "babel-plugin-styled-components",
    "ramda",
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "alias": {
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
