{
  "name": "@tailor-ui/lab",
  "description": "Incubator for Tailor-UI React components.",
  "license": "MIT",
  "author": "dev@yoctol.com",
  "homepage": "https://tailor-ui.netlify.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoctol/tailor-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Yoctol/tailor-ui/issues"
  },
  "version": "0.13.0",
  "main": "lib/index.cjs.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --config-file ../../babel.config.js ./src --out-dir ./lib --extensions '.ts,.tsx'",
    "postbuild": "rollup -c ./config/rollup.config.js",
    "test": "yarn lint && yarn testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@tailor-ui/utils": "^0.2.9",
    "downshift": "3.2.12",
    "fuzzaldrin-plus": "^0.6.0",
    "lodash.debounce": "^4.0.8",
    "polished": "^3.4.2",
    "ramda": "^0.26.1",
    "react-icons": "^3.8.0",
    "react-tiny-virtual-list": "^2.2.0",
    "styled-system": "^4.2.4",
    "tailor-ui": "^0.25.2"
  },
  "peerDependencies": {
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0",
    "react-spring": ">=8.0.0",
    "styled-components": ">=4.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
