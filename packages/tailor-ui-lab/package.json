{
  "name": "@tailor-ui/lab",
  "version": "0.2.4",
  "description": "Incubator for Tailor-UI React components.",
  "author": "Yoctol Info SD Team",
  "homepage": "https://tailor-ui.netlify.com/",
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoctol/tailor-ui.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:types && yarn build:cjs && yarn build:esm",
    "build:types": "tsc --build",
    "build:cjs": "rollup -c scripts/rollup.config.js",
    "build:esm": "cross-env NODE_ENV=production babel --config-file ../../babel.config.js ./src --out-dir ./lib --extensions '.ts,.tsx' --ignore **/__tests__,**/**/__tests__",
    "test": "yarn lint && yarn testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch",
    "type-check:watch": "yarn build:types -- --watch"
  },
  "bugs": {
    "url": "https://github.com/Yoctol/tailor-ui/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-spring": ">=8.0.0",
    "styled-components": ">=4.1.0",
    "tailor-ui": ">=0.4.0"
  },
  "devDependencies": {
    "rollup-plugin-commonjs": "^9.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tailor-ui/utils": "^0.0.1",
    "lodash.debounce": "^4.0.8",
    "polished": "^3.1.0",
    "styled-system": "^4.0.8"
  }
}
