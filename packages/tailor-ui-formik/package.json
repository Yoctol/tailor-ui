    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch"
  },
  "peerDependencies": {
    "formik": ">=2.0.0",
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0",
    "tailor-ui": ">=0.20.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
